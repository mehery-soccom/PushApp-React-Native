<?xml version="1.0" encoding="UTF-8"?>
<document type="com.apple.InterfaceBuilder3.CocoaTouch.Storyboard.XIB" version="3.0" toolsVersion="22522" targetRuntime="iOS.CocoaTouch" propertyAccessControl="none" useAutolayout="YES" useTraitCollections="YES" colorMatched="YES" initialViewController="M4Y-Lb-cyx">
    <device id="retina6_1" orientation="portrait" appearance="light"/>
    <dependencies>
        <plugIn identifier="com.apple.InterfaceBuilder.IBCocoaTouchPlugin" version="22513"/>
        <capability name="Safe area layout guides" minToolsVersion="9.0"/>
    </dependencies>
    <scenes>
        <!-- Notification View Controller -->
        <scene sceneID="cwh-vc-ff4">
            <objects>
                <viewController id="M4Y-Lb-cyx" userLabel="Notification View Controller" customClass="NotificationViewController" customModule="ImagePreviewExtension" customModuleProvider="target" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" simulatedAppContext="notificationCenter" id="RootView">
                        <rect key="frame" x="0" y="0" width="393" height="852"/>
                        <autoresizingMask key="autoresizingMask" widthSizable="YES" heightSizable="YES"/>

                        <!-- FULLY TRANSPARENT ROOT -->
                        <color key="backgroundColor" white="0" alpha="0.0" colorSpace="custom" customColorSpace="genericGamma22GrayColorSpace"/>

                        <subviews>

                            <!-- Transparent Container -->
                            <view contentMode="scaleToFill" translatesAutoresizingMaskIntoConstraints="NO" id="ContainerView">
                                <rect key="frame" x="16" y="16" width="361" height="820"/>

                                <subviews>
                                    
                                    <!-- ImageView: NO background, supports corner radius -->
                                    <imageView id="ImageView"
                                               userInteractionEnabled="NO"
                                               clipsSubviews="YES"
                                               contentMode="scaleAspectFit"
                                               translatesAutoresizingMaskIntoConstraints="NO">
                                        <rect key="frame" x="0" y="0" width="361" height="600"/>
                                    </imageView>

                                    <!-- TextLabel: NO background at all -->
                                    <label id="TextLabel"
                                           text="Sample Text"
                                           textAlignment="center"
                                           numberOfLines="0"
                                           translatesAutoresizingMaskIntoConstraints="NO">
                                        <rect key="frame" x="8" y="608" width="345" height="60"/>
                                        <fontDescription key="fontDescription" type="boldSystem" pointSize="18"/>
                                        <color key="textColor" white="1" alpha="1" colorSpace="custom" customColorSpace="genericGamma22GrayColorSpace"/>
                                    </label>

                                </subviews>

                                <!-- Constraints inside Container -->
                                <constraints>
                                    <!-- Image -->
                                    <constraint firstItem="ImageView" firstAttribute="top" secondItem="ContainerView" secondAttribute="top" id="imgTop"/>
                                    <constraint firstItem="ImageView" firstAttribute="leading" secondItem="ContainerView" secondAttribute="leading" id="imgLeading"/>
                                    <constraint firstAttribute="trailing" secondItem="ImageView" secondAttribute="trailing" id="imgTrailing"/>
                                    <constraint firstItem="ImageView" firstAttribute="height"
                                                secondItem="ContainerView" secondAttribute="width"
                                                multiplier="0.56" id="imgHeightRatio"/>

                                    <!-- Text -->
                                    <constraint firstItem="TextLabel" firstAttribute="top" secondItem="ImageView" secondAttribute="bottom" constant="8" id="labelTop"/>
                                    <constraint firstItem="TextLabel" firstAttribute="leading" secondItem="ContainerView" secondAttribute="leading" constant="8" id="labelLeading"/>
                                    <constraint firstAttribute="trailing" secondItem="TextLabel" secondAttribute="trailing" constant="8" id="labelTrailing"/>
                                    <constraint firstAttribute="bottom" secondItem="TextLabel" secondAttribute="bottom" constant="12" id="labelBottom"/>
                                </constraints>

                            </view>

                        </subviews>

                        <!-- Root constraints -->
                        <constraints>
                            <constraint firstItem="ContainerView" firstAttribute="leading" secondItem="RootView" secondAttribute="leading" constant="16" id="ContainerLeading"/>
                            <constraint firstAttribute="trailing" secondItem="ContainerView" secondAttribute="trailing" constant="16" id="ContainerTrailing"/>
                            <constraint firstItem="ContainerView" firstAttribute="top" secondItem="RootView" secondAttribute="top" constant="16" id="ContainerTop"/>
                            <constraint firstAttribute="bottom" secondItem="ContainerView" secondAttribute="bottom" constant="16" id="ContainerBottom"/>
                        </constraints>

                        <viewLayoutGuide key="safeArea" id="SafeArea"/>
                    </view>

                    <!-- Outlets -->
                    <connections>
                        <outlet property="containerView" destination="ContainerView" id="containerOutlet"/>
                        <outlet property="imageView" destination="ImageView" id="imageOutlet"/>
                        <outlet property="textLabel" destination="TextLabel" id="textOutlet"/>
                    </connections>
                </viewController>

                <placeholder placeholderIdentifier="IBFirstResponder" id="FirstResponder" sceneMemberID="firstResponder"/>
            </objects>
        </scene>
    </scenes>
</document>

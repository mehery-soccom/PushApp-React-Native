{
  "name": "react-native-mehery-event-sender-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme MeheryEventSenderExample --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@firebase/app": "^0.14.1",
    "@firebase/app-types": "^0.9.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/app": "^22.4.0",
    "@react-native-firebase/messaging": "^22.4.0",
    "@react-native/gradle-plugin": "^0.80.2",
    "@react-native/new-app-screen": "0.80.0",
    "@types/node": "^24.2.1",
    "chalk": "4",
    "events": "^3.3.0",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-webview": "^13.15.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@types/react": "^19.1.0",
    "react-native-builder-bob": "^0.38.3",
    "react-native-codegen": "^0.70.7"
  },
  "engines": {
    "node": ">=18"
  }
}
